    "jest-cli": "^19.0.2",
    "poi": "^8.0.0-rc.4",
    "postcss-nested": "^1.0.1",
    "text-spinners": "^1.0.5",
    "xo": "^0.18.1"
  },
  "xo": {
    "extends": "rem",
    "envs": [
      "browser"
    ]
  },
  "jest": {
    "testEnvironment": "node"
